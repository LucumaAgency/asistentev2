{
  "name": "ai-assistant-plesk",
  "version": "3.40.0",
  "description": "AI Assistant Web Application with Voice Support",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:cjs": "node server.cjs",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server.js",
    "dev:client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "assistant",
    "voice",
    "openai",
    "mariadb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.2.1",
    "googleapis": "^155.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.24.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
